<div class="flex space-x-4 mb-6">
  <div
    role="img"
    class="relative"
    aria-label="State Available and  the User">
    <img
      class="w-10 h-10 rounded-full overflow-hidden"
      width="40"
      height="40"
      src="/src/assets/avatar.jpg"
      aria-hidden="true"
      alt="Display"
    />
    <span
      class="absolute top-0 left-7 w-3 h-3 bg-green-600 dark:bg-green-400 rounded-full"
      aria-hidden="true"
    ></span>
  </div>

  <div
    role="img"
    aria-label="State Busy and The User Placeholder"
    class="relative">
    <svg
      xmlns="http://www.w3.org/2000/svg"
      width="40"
      height="40"
      viewBox="0 0 24 24"
      class="w-10 h-10 overflow-hidden bg-gray-400 rounded-full dark:bg-gray-800 text-indigo-600 dark:text-indigo-400"
      aria-hidden="true">
      <path
        fill="currentColor"
        d="M12 12c2.21 0 4-1.79 4-4s-1.79-4-4-4s-4 1.79-4 4s1.79 4 4 4m0 2c-2.67 0-8 1.34-8 4v2h16v-2c0-2.66-5.33-4-8-4"
      ></path>
    </svg>
    <span
      class="absolute top-0 left-7 w-3 h-3 bg-red-600 dark:bg-red-500 rounded-full"
      aria-hidden="true"
    ></span>
  </div>

  <div
    role="img"
    aria-label="State Away and The User Initials RX"
    class="relative inline-flex items-center justify-center h-10 w-10 bg-gray-400 rounded-full dark:bg-gray-800">
    <span
      aria-hidden="true"
      class="text-xl font-semibold leading-none text-indigo-600 dark:text-indigo-400"
      >RX</span
    >
    <span
      class="absolute top-0 left-7 w-3 h-3 bg-yellow-500 dark:bg-yellow-400 rounded-full"
      aria-hidden="true"
    ></span>
  </div>
</div>
<div class="flex space-x-4">
  <a
    href="#"
    aria-label="To Profile Page With State Available and The User Display Image"
    title="To Profile Page"
    class="relative cursor-pointer w-10 h-10 rounded-full ">
    <img
      class="inline-block overflow-hidden rounded-full"
      src="/src/assets/avatar.jpg"
      aria-hidden="true"
      alt="To Profile Page"
      title="To Profile Page"
      width="40"
      height="40"
    />
    <span
      class="absolute top-0 left-7 w-3 h-3 bg-green-600 dark:bg-green-400 rounded-full"
      aria-hidden="true"
    ></span>
  </a>
  <a
    href="#"
    aria-label="To Profile Page With State Busy and The User Display Image Placeholder"
    title="To Profile Page"
    class="relative cursor-pointer w-10 h-10  bg-gray-400 rounded-full dark:bg-gray-800">
    <svg
      xmlns="http://www.w3.org/2000/svg"
      width="40"
      height="40"
      viewBox="0 0 24 24"
      class="text-indigo-600 dark:text-indigo-400"
      aria-hidden="true">
      <path
        fill="currentColor"
        d="M12 12c2.21 0 4-1.79 4-4s-1.79-4-4-4s-4 1.79-4 4s1.79 4 4 4m0 2c-2.67 0-8 1.34-8 4v2h16v-2c0-2.66-5.33-4-8-4"
      ></path>
    </svg>
    <span
      class="absolute top-0 left-7 w-3 h-3 bg-red-600 dark:bg-red-500 rounded-full"
      aria-hidden="true"
    ></span>
  </a>
  <a
    href="#"
    aria-label="To Profile Page With State Away and The User Initials RX"
    title="To Profile Page"
    class="relative cursor-pointer inline-flex items-center justify-center h-10 w-10 bg-gray-400 rounded-full dark:bg-gray-800">
    <span
      aria-hidden="true"
      class="text-xl font-semibold leading-none text-indigo-600 dark:text-indigo-400"
      >RX</span
    >
    <span
      class="absolute top-0 left-7 w-3 h-3 bg-yellow-500 dark:bg-yellow-400 rounded-full"
      aria-hidden="true"
    ></span>
  </a>
</div>
